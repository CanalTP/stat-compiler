<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="db.dsn" type="string">pgsql:host=localhost;port=5432;dbname=statistics</parameter>
        <parameter key="db.user" type="string">postgres</parameter>
        <parameter key="db.password" type="string"></parameter>

        <parameter key="logger.loglevel" type="constant">Monolog\Logger::INFO</parameter>
        <parameter key="logger.file" type="string">/tmp/stat_compiler.log</parameter>
        <parameter key="logger.maxfiles" type="string">7</parameter>
        <parameter key="logger.handler" type="string">log.syslog_handler</parameter>
    </parameters>
</container>
